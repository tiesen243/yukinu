{
  "name": "@yukinu/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/lib/utils.mjs",
    "./*": "./dist/components/*.mjs",
    "./hooks/*": "./dist/hooks/*.mjs",
    "./tailwind.css": "./src/tailwind.css",
    "./package.json": "./package.json"
  },
  "main": "./dist/utils.mjs",
  "module": "./dist/utils.mjs",
  "types": "./dist/utils.d.mts",
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch ./src",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tailwindcss": "catalog:tailwind",
    "tsdown": "catalog:",
    "tw-animate-css": "^1.4.0",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@types/react": "catalog:react",
    "prettier-plugin-tailwindcss": "catalog:",
    "react": "catalog:react"
  }
}

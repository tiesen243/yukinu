{
  "name": "@yukinu/validators",
  "version": "0.1.6",
  "private": true,
  "type": "module",
  "exports": {
    "./address": "./dist/address.mjs",
    "./auth": "./dist/auth.mjs",
    "./env": "./dist/env.mjs",
    "./env.next": "./dist/env.next.mjs",
    "./env.vite": "./dist/env.vite.mjs",
    "./profile": "./dist/profile.mjs",
    "./user": "./dist/user.mjs",
    "./vendor": "./dist/vendor.mjs",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "~dev": "tsdown --watch ./src"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@types/node": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  }
}

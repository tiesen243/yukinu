{
  "name": "@yukinu/validators",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./*": {
      "default": "./dist/*.mjs",
      "types": "./dist/*.d.mts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsdown --watch ./src",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@types/node": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts"
}

  "devDependencies": {
    "@react-email/preview-server": "5.0.8",
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react-email": "^5.0.8",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@types/react": "catalog:react",
    "react": "catalog:react"
  }

{
  "name": "@yukinu/auth",
  "version": "0.9.0",
  "private": true,
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "react-server": "./dist/index.rsc.mjs",
      "default": "./dist/index.mjs"
    },
    "./react": {
      "types": "./dist/react.d.mts",
      "default": "./dist/react.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit",
    "~dev": "tsdown --watch ./src"
  },
  "dependencies": {
    "@yukinu/db": "workspace:*",
    "@yukinu/email": "workspace:*",
    "@yukinu/lib": "workspace:*",
    "@yukinu/validators": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:dev",
    "@yukinu/tsconfig": "workspace:*",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev"
  },
  "peerDependencies": {
    "@tanstack/react-query": "catalog:prod",
    "@types/react": "catalog:react",
    "react": "catalog:react"
  }
}

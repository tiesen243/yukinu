{
  "name": "@yukinu/auth",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "react-server": "./dist/index.rsc.mjs",
      "default": "./dist/index.mjs"
    },
    "./react": {
      "types": "./dist/react.d.mts",
      "default": "./dist/react.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "~dev": "tsdown --watch ./src"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@yukinu/db": "workspace:*",
    "@yukinu/validators": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@tanstack/react-query": "catalog:",
    "@types/react": "catalog:react",
    "react": "catalog:react"
  }
}

  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@trpc/server": "catalog:trpc",
    "@yukinu/auth": "workspace:*",
    "@yukinu/db": "workspace:*",
    "@yukinu/email": "workspace:*",
    "@yukinu/lib": "workspace:*",
    "@yukinu/validators": "workspace:*",
    "superjson": "catalog:trpc"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  }
}

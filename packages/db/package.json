{
  "name": "@yukinu/db",
  "version": "0.0.4",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./mock": {
      "types": "./dist/index.mock.d.ts",
      "default": "./src/index.mock.ts"
    },
    "./schema/*": {
      "types": "./dist/schema/*.d.ts",
      "default": "./src/schema/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "db:generate": "bun run --env-file ../../.env drizzle-kit generate",
    "db:migrate": "bun run --env-file ../../.env drizzle-kit migrate",
    "db:push": "bun run --env-file ../../.env drizzle-kit push",
    "db:studio": "bun run --env-file ../../.env drizzle-kit studio",
    "dev": "tsc --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@yukinu/validators": "workspace:*",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "drizzle-kit": "^0.31.6",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@types/node": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  }
}

{
  "name": "@yukinu/db",
  "version": "0.0.6",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./schemas/*": {
      "types": "./dist/schemas/*.d.ts",
      "default": "./src/schemas/*.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "db:generate": "bun run with-env drizzle-kit generate",
    "db:migrate": "bun run with-env drizzle-kit migrate",
    "db:push": "bun run with-env drizzle-kit push",
    "db:studio": "bun run with-env drizzle-kit studio",
    "dev": "tsc --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@yukinu/validators": "workspace:*",
    "drizzle-orm": "^0.44.6",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "dotenv-cli": "catalog:",
    "drizzle-kit": "^0.31.5",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  }
}

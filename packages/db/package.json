{
  "name": "@yukinu/db",
  "version": "0.1.3",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json",
    "./schema/*": {
      "default": "./dist/schema/*.mjs",
      "types": "./dist/schema/*.d.mts"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsdown",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "db:generate": "bun run --env-file ../../.env drizzle-kit generate",
    "db:migrate": "bun run --env-file ../../.env drizzle-kit migrate",
    "db:push": "bun run --env-file ../../.env drizzle-kit push",
    "db:studio": "bun run --env-file ../../.env drizzle-kit studio",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "~dev": "tsdown --watch ./src"
  },
  "prettier": "@yukinu/prettier-config",
  "dependencies": {
    "@yukinu/validators": "workspace:*",
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@yukinu/eslint-config": "workspace:*",
    "@yukinu/prettier-config": "workspace:*",
    "@yukinu/tsconfig": "workspace:*",
    "drizzle-kit": "^0.31.6",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "catalog:",
    "@types/node": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:"
  }
}

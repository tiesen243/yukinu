{
  "name": "yukinu",
  "private": true,
  "homepage": "https://yukinu.vercel.app",
  "bugs": {
    "url": "https://github.com/tiesen243/yukinu/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiesen243/yukinu.git"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && git clean -xdf node_modules",
    "dev": "turbo dev --continue",
    "format": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "turbo": "^2.7.5"
  },
  "packageManager": "bun@1.3.0",
  "catalogs": {
    "prod": {
      "@tanstack/react-query": "^5.90.19",
      "nuqs": "^2.8.6",
      "zod": "4.3.6"
    },
    "dev": {
      "@types/node": "^25.0.10",
      "tsdown": "^0.20.0",
      "typescript": "^5.9.3"
    },
    "react": {
      "@types/react": "^19.2.9",
      "@types/react-dom": "^19.2.3",
      "react": "19.1.0",
      "react-dom": "19.1.0"
    },
    "tailwind": {
      "@tailwindcss/postcss": "^4.1.18",
      "@tailwindcss/vite": "^4.1.18",
      "tailwindcss": "^4.1.18",
      "uniwind": "^1.2.6"
    },
    "uploadthing": {
      "@uploadthing/expo": "^7.2.6",
      "@uploadthing/react": "^7.3.3",
      "@uploadthing/shared": "^7.1.10",
      "uploadthing": "^7.7.4"
    },
    "trpc": {
      "@trpc/server": "^11.8.1",
      "@trpc/client": "^11.8.1",
      "@trpc/tanstack-react-query": "^11.8.1",
      "superjson": "^2.2.6"
    }
  }
}

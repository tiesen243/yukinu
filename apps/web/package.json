    "@yukinu/uploadthing": "workspace:*",
    "@yukinu/validators": "workspace:*",
    "next": "catalog:react",
    "nuqs": "catalog:prod",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "superjson": "catalog:trpc"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "catalog:tailwind",
    "@types/node": "catalog:dev",
    "@types/react": "catalog:react",

{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "yukinu-web": {
      "packageName": "@yukinu/web",
      "development": {
        "local": 3000,
        "fallback": "yukinu.vercel.app"
      }
    },
    "yukinu-dashboard": {
      "packageName": "@yukinu/dashboard",
      "routing": [{ "paths": ["/dashboard/:path*"] }],
      "development": {
        "local": 5173,
        "fallback": "yukinu-dashboard.vercel.app"
      }
    }
  }
}
